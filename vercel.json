{
    "version": 2,
    "name": "my-nextjs-app",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      },
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "api/index.js" },
      { "src": "/(.*)", "dest": "/$1" }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }
  